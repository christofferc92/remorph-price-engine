{
  "name": "bathroom-renovation-estimator",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -k --names \"server,client\" --prefix \"[{name}]\" \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "sh -lc 'mkdir -p .tmp && TMPDIR=\"$PWD/.tmp\" TSX_DISABLE_DAEMON=1 tsx server/server.ts'",
    "client:dev": "tsx scripts/dev-client.ts",
    "build": "vite build",
    "preview": "vite preview",
    "start": "sh -lc 'mkdir -p .tmp && TMPDIR=\"$PWD/.tmp\" TSX_DISABLE_DAEMON=1 NODE_ENV=production tsx server/server.ts'",
    "test": "sh -lc 'mkdir -p .tmp && TMPDIR=\"$PWD/.tmp\" TSX_DISABLE_DAEMON=1 tsx scripts/run-tests.ts'",
    "test:api-floor-heating": "sh -lc 'mkdir -p .tmp && TMPDIR=\"$PWD/.tmp\" NODE_ENV=test TS_NODE_PROJECT=tsconfig.json ts-node --esm --compiler-options \"{\\\"allowImportingTsExtensions\\\":true,\\\"noImplicitAny\\\":false}\" scripts/api-estimate-floor-heating-test.ts'",
    "sweep:validate": "NODE_ENV=test node --experimental-strip-types server/tools/sweepValidate.ts",
    "sweep:baseline": "NODE_ENV=test node --experimental-strip-types server/tools/sweepValidate.ts --write-baseline",
    "sweep:diff": "NODE_ENV=test node --experimental-strip-types server/tools/sweepValidate.ts --diff-baseline",
    "sweep:gate": "NODE_ENV=test node --experimental-strip-types server/tools/sweepValidate.ts --diff-baseline --fail-on-new-outliers=1 --fail-on-delta-mid-p95-abs=500",
    "report:quality": "node --experimental-strip-types server/tools/qualityReport.ts",
    "price-engine:dev": "TSX_DISABLE_DAEMON=1 tsx apps/price-engine-service/src/server.ts",
    "price-engine:start": "TSX_DISABLE_DAEMON=1 NODE_ENV=production tsx apps/price-engine-service/src/server.ts",
    "price-engine:equivalence-test": "TSX_DISABLE_DAEMON=1 tsx scripts/equivalence-test.ts",
    "smoke-ai-offert": "TSX_DISABLE_DAEMON=1 tsx scripts/smoke-ai-offert.ts",
    "smoke-ai-after-image": "TSX_DISABLE_DAEMON=1 tsx scripts/smoke-ai-after-image.ts",
    "generate:price-copy-catalog": "TSX_DISABLE_DAEMON=1 tsx scripts/generate-price-copy-catalog.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.93.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^6.15.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "yaml": "^2.6.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.7",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.2",
    "concurrently": "^8.2.2",
    "node-mocks-http": "^1.17.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^6.0.1"
  }
}
